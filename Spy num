import java.util.Scanner;

public class Spy {

    public static int issum(int temp) {

        int num = temp;
        int sum = 0;
        int rem;
        while (num > 0) {

            rem = num % 10;
            sum = sum + rem;
            num = num / 10;
        }
        return sum;

    }

    public static int mul(int temp) {
        int mul = 1;
        int rem = 0;
        while (temp > 0) {

            rem = temp % 10;
            mul = mul * rem;
            temp = temp / 10;

        }
        return mul;

    }

    public static boolean isSpy(int temp) {

        return issum(temp) == mul(temp);

    }

    public static void isRange(int temp) {

        for (int i = 0; i < temp; i++) {
            if (isSpy(i)) {
                System.out.println(i);

            }

        }
    }

    public static void main(String[] args) {
        System.out.println(isSpy(1124));
        isRange(1500);
    }
}

import java.util.Scanner;

public class Armstrong1 {

    public static int iscube(int rem) {
        return rem * rem * rem;

    }

    public static int isSum(int temp) {

        int sum = 0;

        int num = temp;

        while (num > 0) {
            int rem = num % 10;
            sum = sum + iscube(rem);
            num = num / 10;
        }
        return sum;
    }

    public static boolean isArm(int temp) {
        return isSum(temp) == temp;

    }

    public static void Range(int temp) {
        for (int i = 1; i < 1000; i++) {
            if (isArm(i)) {
                System.out.println(i);

            }

        }
    }

    public static void main(String[] args) {

        System.out.println(isArm(153));
        Range(1000);
    }
}

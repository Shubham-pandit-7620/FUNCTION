
import java.util.Scanner;

public class Harshad {

    public static int sum(int temp) {

        int num = temp;
        int sum = 0;
        int rem = 0;

        while (num > 0) {
            rem = num % 10;
            sum = sum + rem;
            num = num / 10;
        }
        return sum;

    }

    public static boolean isHarshad(int temp) {
        return temp % sum(temp) == 0;
    }

    public static void Range(int temp) {

        for (int i = 1; i <= temp; i++) {
            if (isHarshad(i)) {
                System.out.println(i);

            }

        }
    }

    public static void main(String[] args) {

        System.out.println(isHarshad(18));
        Range(1000);

    }

}
